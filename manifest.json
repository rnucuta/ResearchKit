{
	"manifest_version": 2,
	"name": "ResearchKit",
	"version": "1.2",
	"description": "Research quickly on the go!",
	"icons": {
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": true
	},
	"browser_action": {
    	"default_icon": "icon48.png"
  	},
	"permissions": [
		"tabs",
		"contextMenus"
	]
}